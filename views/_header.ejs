<header id="page_header">
    <p>CODEIAL</p>
    <ul id="links">
        <% if(locals.user){ %> 
            <li>
                <a href="/user/profile/<%= locals.user.id %>"><%= user.name %> </a>
            </li>
            <li>
                <a href="/user/sign-out">SignOut</a>
            </li>
        <% }else{ %> 
            <li>
                <a href="/user/signin">SignIn</a>
            </li>
            <li>
                <a href="/user/signup">SignUp</a>
            </li>
        <% } %> 
    </ul>
</header>